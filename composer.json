{
    "name": "rudra/exception",
    "description": "Rudra framework",
    "type": "library",
    "keywords": [
        "rudra",
        "exception",
        "exceptions"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jagepard",
            "email": "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "jagepard@yandex.ru",
        "issues": "https://github.com/Jagepard/Rudra-Exceptions/issues", 
        "source": "https://github.com/Jagepard/Rudra-Exceptions" 
    },
    "require": {
        "php": ">=8.3",
        "rudra/container": "v25.10",
        "rudra/redirect": "v25.10",
        "rudra/docs": "v25.10"
    },
    "autoload": {
        "psr-4": {
            "Rudra\\Exceptions\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rudra\\Tests\\" : "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
