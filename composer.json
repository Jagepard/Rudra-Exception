{
    "name"              : "rudra/exception",
    "description"       : "Rudra framework",
    "type"              : "library",
    "keywords"          : [
        "rudra",
        "exception",
        "exceptions"
    ],
    "license"           : "MIT",
    "authors"           : [
        {
            "name"     : "Jagepard",
            "email"    : "jagepard@yandex.ru",
            "homepage" : "https://jagepard.ru",
            "role"     : "Developer"
        }
    ],
    "support": {
        "email": "jagepard@yandex.ru",
        "issues": "https://github.com/Jagepard/Rudra-Exceptions/issues", 
        "source": "https://github.com/Jagepard/Rudra-Exceptions" 
    },
    "require"           : {
        "php"               : ">=7.4",
        "rudra/container"   : "self.version",
        "rudra/redirect"    : "self.version",
        "rudra/docs": "dev-master"
    },
    "autoload"          : {
        "psr-4" : {
            "Rudra\\Exceptions\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "Rudra\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "config"            : {
        "optimize-autoloader" : true,
        "prefer-stable"       : true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
